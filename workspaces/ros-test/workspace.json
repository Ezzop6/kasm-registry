{
  "description": "Ubuntu with ROS Noetic desktop",
  "docker_registry": "https://hub.docker.com/",
  "image_src": "chromium.png",
  "name": "ezzop6/kasm-ros-noetic:latest",
  "run_config": {
    "hostname": "kasm"
  },
  "exec_config": {
    "go": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --go --url \"$KASM_URL\"'"
    },
    "assign": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --assign --url \"$KASM_URL\"'"
    }
  },
  "exec_config": {},
  "categories": ["Desktop"],
  "friendly_name": "ROS Noetic Ubuntu 20.04",
  "architecture": ["amd64"],
  "compatibility": ["1.15.x"],
  "uncompressed_size_mb": 2200
}
